{
  "name": "metamodels/attribute_timestamp",
  "description": "MetaModels attribute for timestamps",
  "keywords": [
    "contao",
    "metamodels",
    "attribute",
    "timestamp"
  ],
  "type": "contao-module",
  "homepage": "http://now.metamodel.me/",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Christian Schiffler",
      "email": "c.schiffler@cyberspectrum.de",
      "homepage": "http://www.cyberspectrum.de",
      "role": "Developer"
    },
    {
      "name": "Stefan Heimes",
      "email": "stefan_heimes@hotmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "mail@metamodel.me",
    "issues": "https://github.com/MetaModels/attribute_timestamp/issues",
    "wiki": "http://de.contaowiki.org/MetaModels",
    "irc": "irc://irc.freenode.org/contao.mm",
    "source": "https://github.com/MetaModels/attribute_timestamp"
  },
  "require": {
    "php": ">=5.3",
    "contao/core": ">=2.11,<4.0",
    "contao-community-alliance/composer-plugin": "~2.0",
    "metamodels/core": "~2.0",
    "metamodels/attribute_numeric": "~2.0",
    "contao-community-alliance/events-contao-bindings": "*"
  },
  "require-dev": {
    "cyberspectrum/contao-toolbox": "~0.6",
    "phpunit/phpunit": "~3.7",
    "sebastian/phpcpd": "~1.4",
    "phploc/phploc": "~2.0",
    "phpmd/phpmd": "~2.0",
    "squizlabs/php_codesniffer": "~1.0",
    "contao-community-alliance/build-system-tool-branch-alias-validation": "~1.0",
    "contao-community-alliance/build-system": "~1.0",
    "contao-community-alliance/coding-standard": "~1.0",
    "metamodels/base-unit-tests": "~1.0"
  },
  "conflict": {
    "contao/core": "3.0.*"
  },
  "autoload": {
    "psr-0": {
      "MetaModels": "src"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "MetaModels\\Test": "tests"
    }
  },
  "extra": {
    "contao": {
      "sources": {
        "contao": "system/modules/metamodelsattribute_timestamp"
      },
      "transifex": {
        "project": "metamodels",
        "prefix": "attribute_timestamp-",
        "languages_cto": "contao/languages",
        "languages_tx": ".tx"
      }
    },
    "branch-alias": {
      "dev-tng": "2.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
